<h3>Posts</h3>

<table>
<thead>
<tr>
<th>title</th>
<th>body</th>
<th></th>
</tr>
</thead>

<tbody>
<% @posts.each do |post| %>

<tr>
<td>
<%= post.title %>
</td>
<td>
<%= post.caption %>
</td>
<td>


<%= attachment_image_tag post, :post_image %>


<%= link_to "show", post_path(post.id) %>
<%= link_to "edit", edit_post_path(post.id) %>

</td>
</tr>
<% end %>
</tbody>
</table>

